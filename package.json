{
  "name": "babel-plugin-import-bang-text",
  "version": "0.0.1",
  "description": "Babel plugin that supports `import foo from 'foo.txt!text'`",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --copy-files",
    "clean": "rm -rf lib",
    "lint": "eslint .",
    "test": "npm run lint && mocha --compilers js:babel-register",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onlywei/babel-plugin-import-bang-text.git"
  },
  "keywords": [
    "babel-plugin", "SystemJS"
  ],
  "author": "Wei Wang <wei@fastmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onlywei/babel-plugin-import-bang-text/issues"
  },
  "homepage": "https://github.com/onlywei/babel-plugin-import-bang-text#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "eslint": "^1.10.3",
    "mocha": "^2.3.4"
  }
}
